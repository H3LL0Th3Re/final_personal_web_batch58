{
    "version": 2,
    "builds": [
        { "src": "router.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/login", "dest": "router.js" },
        { "src": "/register", "dest": "router.js" },
        { "src": "/assets/(.*)", "dest": "/assets/$1" },  // Serve static assets
        { "src": "/(.*)", "dest": "router.js" }
    ]
}

  
